<h1>Sign in</h1>
<%= form_tag sessions_path do %>
    <table>
     <tr>
          <td><%= label_tag :email %></td>
            <td><%= text_field_tag :email %>
        </tr>
        <tr>
          <td><%= label_tag :password %></td>
           <td><%= password_field_tag :password %>
        </tr>
     <tr>
          <td><%= submit_tag "Sign in" %></td>
        </tr>
   </table>
<% end %>
<p>Not registered yet? <%= link_to 'Create an account!', new_user_path %></p>
<p>Find me in app/views/sessions/new.html.erb</p>
